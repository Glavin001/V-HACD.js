{
  "name": "v-hacd",
  "version": "0.1.0",
  "description": "Voxelized Hierarchical Convex Decomposition (V-HACD) for JavaScript/Web",
  "source": "src/index.ts",
  "main": "dist/v-hacd.cjs.js",
  "module": "dist/v-hacd.esm.js",
  "types": "dist/v-hacd.cjs.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node -e \"import('./scripts/test.js')\"",
    "build": "npm run build:lib && npm run types",
    "build:lib": "yarn patch:lib && docker-compose up --build --exit-code-from lib lib",
    "patch:lib": "cd src/lib/v-hacd && git apply ../patches/VHACD.patch || true",
    "types": "cp ./src/ammo.d.ts ./builds/ammo.d.ts"
  },
  "repository": "https://github.com/Glavin001/V-HACD.js/tree/master/packages/v-hacd",
  "keywords": [
    "v-hacd",
    "hacd"
  ],
  "author": "Glavin Wiechert <glavin.wiechert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glavin001/V-HACD.js/issues"
  },
  "homepage": "https://github.com/Glavin001/V-HACD.js/tree/master/packages/v-hacd#readme",
  "devDependencies": {
    "webidl2ts": "github:glavin001/webidl2ts"
  }
}
